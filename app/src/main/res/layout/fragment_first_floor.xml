                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/a35"
                        android:layout_width="50dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/a34"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/a36"
                        android:layout_width="50dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/a35"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                </androidx.constraintlayout.widget.ConstraintLayout>

                <!-- map 3 -->
                <ImageView
                    android:id="@+id/map3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:background="@drawable/line_30_36"
                    android:backgroundTint="@color/white"
                    android:contentDescription="@string/map3" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <!-- g3 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/entrance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:background="@drawable/entrance"
                    android:contentDescription="@string/entrance" />

            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

<!-- Button -->
    <Button
        android:id="@+id/button"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/parkmap"
