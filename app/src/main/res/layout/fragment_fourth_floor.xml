<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="?attr/bgcf"
    tools:context=".view.fragments.FourthFloor">

    <!-- color code -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/colorcode"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp">

        <!-- color 1 -->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/color1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent">

            <ImageView
                android:id="@+id/cchosen"
                android:layout_width="30dp"
                android:layout_height="30dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:background="@color/hawaiian_passion"
                android:contentDescription="@string/color1" />

            <TextView
                android:id="@+id/tchosen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/cchosen"
                android:layout_marginStart="10dp"
                android:fontFamily="@font/product_sans_bold"
                android:textSize="12sp"
                android:textColor="@color/lynx_white"
                android:text="@string/choosen"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <!-- color 2 -->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/color2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/color1"
            app:layout_constraintEnd_toStartOf="@id/color3"
            app:layout_constraintBottom_toBottomOf="parent">

            <ImageView
                android:id="@+id/cava"
                android:layout_width="30dp"
                android:layout_height="30dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:background="@color/bluish_cyan"
                android:contentDescription="@string/color1" />

            <TextView
                android:id="@+id/tava"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/cava"
                android:layout_marginStart="10dp"
                android:fontFamily="@font/product_sans_bold"
                android:textSize="12sp"
                android:textColor="@color/lynx_white"
                android:text="@string/available"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <!-- color 3 -->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/color3"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <ImageView
                android:id="@+id/cfill"
                android:layout_width="30dp"
                android:layout_height="30dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:background="@color/shuttle_grey"
                android:contentDescription="@string/color1" />

            <TextView
                android:id="@+id/tfill"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/cfill"
                android:layout_marginStart="10dp"
                android:fontFamily="@font/product_sans_bold"
                android:textSize="12sp"
                android:textColor="@color/lynx_white"
                android:text="@string/filled"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!-- Parking Map -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/parkmap"
        android:layout_width="match_parent"
        android:layout_height="500dp"
        app:layout_constraintTop_toBottomOf="@id/colorcode"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="40dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp">

        <!-- gorup 1 -->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/g1"
            android:layout_width="84dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <ImageView
                android:id="@+id/map1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:background="@drawable/line_1_11"
                android:backgroundTint="@color/white"
                android:contentDescription="@string/map1" />

            <FrameLayout
                android:id="@+id/d1"
                android:layout_width="49dp"
                android:layout_height="24dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:background="@color/bluish_cyan"/>

            <FrameLayout
                android:id="@+id/d2"
                android:layout_width="49dp"
                android:layout_height="24dp"
                app:layout_constraintBottom_toTopOf="@id/d1"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginBottom="34dp"
                android:background="@color/bluish_cyan"/>

            <FrameLayout
                android:id="@+id/d3"
                android:layout_width="49dp"
                android:layout_height="24dp"
                app:layout_constraintBottom_toTopOf="@id/d2"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginBottom="6dp"
                android:background="@color/bluish_cyan"/>

            <FrameLayout
                android:id="@+id/d4"
                android:layout_width="49dp"
                android:layout_height="24dp"
                app:layout_constraintBottom_toTopOf="@id/d3"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginBottom="6dp"
                android:background="@color/bluish_cyan"/>

            <FrameLayout
                android:id="@+id/d5"
                android:layout_width="49dp"
                android:layout_height="24dp"
                app:layout_constraintBottom_toTopOf="@id/d4"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginBottom="6dp"
                android:background="@color/bluish_cyan"/>

            <FrameLayout
                android:id="@+id/d6"
                android:layout_width="49dp"
                android:layout_height="24dp"
                app:layout_constraintBottom_toTopOf="@id/d5"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginBottom="6dp"
                android:background="@color/bluish_cyan"/>

            <FrameLayout
                android:id="@+id/d7"
                android:layout_width="49dp"
                android:layout_height="24dp"
                app:layout_constraintBottom_toTopOf="@id/d6"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginBottom="6dp"
                android:background="@color/bluish_cyan"/>

            <FrameLayout
                android:id="@+id/d8"
                android:layout_width="49dp"
                android:layout_height="24dp"
                app:layout_constraintBottom_toTopOf="@id/d7"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginBottom="6dp"
                android:background="@color/bluish_cyan"/>

            <FrameLayout
                android:id="@+id/d9"
                android:layout_width="49dp"
                android:layout_height="24dp"
                app:layout_constraintBottom_toTopOf="@id/d8"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginBottom="6dp"
                android:background="@color/bluish_cyan"/>

            <FrameLayout
                android:id="@+id/d10"
                android:layout_width="49dp"
                android:layout_height="24dp"
                app:layout_constraintBottom_toTopOf="@id/d9"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginBottom="6dp"
                android:background="@color/bluish_cyan"/>

            <FrameLayout
                android:id="@+id/d11"
                android:layout_width="49dp"
                android:layout_height="24dp"
                app:layout_constraintBottom_toTopOf="@id/d10"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginBottom="34dp"
                android:background="@color/bluish_cyan"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <!-- group 2 -->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/g2"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/g1"
            app:layout_constraintEnd_toStartOf="@id/g3">

            <!-- 41 - 44 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/top"
                android:layout_width="111dp"
                android:layout_height="83dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent">

                <ImageView
                    android:id="@+id/map5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:background="@drawable/line_41_44"
                    android:backgroundTint="@color/white"
                    android:contentDescription="@string/map5" />

                <FrameLayout
                    android:id="@+id/d41"
                    android:layout_width="24dp"
                    android:layout_height="49dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/d42"
                    android:background="@color/bluish_cyan"/>

                <FrameLayout
                    android:id="@+id/d42"
                    android:layout_width="24dp"
                    android:layout_height="49dp"
                    app:layout_constraintStart_toEndOf="@id/d41"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/d43"
                    android:layout_marginStart="5dp"
                    android:background="@color/bluish_cyan"/>

                <FrameLayout
                    android:id="@+id/d43"
                    android:layout_width="24dp"
                    android:layout_height="49dp"
                    app:layout_constraintStart_toEndOf="@id/d42"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/d44"
                    android:layout_marginStart="5dp"
                    android:background="@color/bluish_cyan"/>

                <FrameLayout
                    android:id="@+id/d44"
                    android:layout_width="24dp"
                    android:layout_height="49dp"
                    app:layout_constraintStart_toEndOf="@id/d43"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginStart="5dp"
                    android:background="@color/bluish_cyan"/>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <!-- 12-29 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/mid"
                android:layout_width="140dp"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/top"
                app:layout_constraintBottom_toTopOf="@id/bot"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginTop="35dp"
                android:layout_marginBottom="35dp">

                <!-- 12-20  -->
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/left"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent">

                    <FrameLayout
                        android:id="@+id/d12"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d13"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintBottom_toTopOf="@id/d12"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginBottom="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d14"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintBottom_toTopOf="@id/d13"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginBottom="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d15"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintBottom_toTopOf="@id/d14"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginBottom="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d16"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintBottom_toTopOf="@id/d15"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginBottom="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d17"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintBottom_toTopOf="@id/d16"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginBottom="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d18"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintBottom_toTopOf="@id/d17"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginBottom="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d19"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintBottom_toTopOf="@id/d18"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginBottom="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d20"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintBottom_toTopOf="@id/d19"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginBottom="6dp"
                        android:background="@color/bluish_cyan"/>

                </androidx.constraintlayout.widget.ConstraintLayout>

                <ImageView
                    android:id="@+id/map2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@id/left"
                    app:layout_constraintEnd_toStartOf="@id/right"
                    android:background="@drawable/line_12_29"
                    android:backgroundTint="@color/white"
                    android:contentDescription="@string/map2" />

                <!-- 21-30  -->
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/right"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent">

                    <FrameLayout
                        android:id="@+id/d21"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d22"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/d21"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d23"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/d22"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d24"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/d23"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d25"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/d24"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d26"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/d25"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d27"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/d26"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d28"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/d27"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d29"
                        android:layout_width="49dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/d28"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <!-- 37-40 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/bot"
                android:layout_width="111dp"
                android:layout_height="83dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent">

                <FrameLayout
                    android:id="@+id/d37"
                    android:layout_width="24dp"
                    android:layout_height="49dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/d38"
                    app:layout_constraintTop_toTopOf="parent"
                    android:background="@color/bluish_cyan"/>

                <FrameLayout
                    android:id="@+id/d38"
                    android:layout_width="24dp"
                    android:layout_height="49dp"
                    app:layout_constraintStart_toEndOf="@id/d37"
                    app:layout_constraintEnd_toStartOf="@id/d39"
                    app:layout_constraintTop_toTopOf="parent"
                    android:background="@color/bluish_cyan"
                    android:layout_marginStart="5dp"/>

                <FrameLayout
                    android:id="@+id/d39"
                    android:layout_width="24dp"
                    android:layout_height="49dp"
                    app:layout_constraintStart_toEndOf="@id/d38"
                    app:layout_constraintEnd_toStartOf="@id/d40"
                    app:layout_constraintTop_toTopOf="parent"
                    android:background="@color/bluish_cyan"
                    android:layout_marginStart="5dp"/>

                <FrameLayout
                    android:id="@+id/d40"
                    android:layout_width="24dp"
                    android:layout_height="49dp"
                    app:layout_constraintStart_toEndOf="@id/d39"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:background="@color/bluish_cyan"
                    android:layout_marginStart="5dp"/>

                <ImageView
                    android:id="@+id/map4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:background="@drawable/line_37_40"
                    android:backgroundTint="@color/white"
                    android:contentDescription="@string/map4" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <!-- group 3 -->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/g3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <!-- g1 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/exit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/exit"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:contentDescription="@string/exit" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <!-- g2 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/mr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/exit"

                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toTopOf="@id/entrance"
                android:layout_marginTop="50dp"
                android:layout_marginBottom="50dp">

                <!-- 30-36 -->
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/bmap3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/map3"
                    android:layout_marginEnd="9dp">

                    <FrameLayout
                        android:id="@+id/d30"
                        android:layout_width="50dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d31"
                        android:layout_width="50dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/d30"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d32"
                        android:layout_width="50dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/d31"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d33"
                        android:layout_width="50dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/d32"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d34"
                        android:layout_width="50dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/d33"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d35"
                        android:layout_width="50dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/d34"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                    <FrameLayout
                        android:id="@+id/d36"
                        android:layout_width="50dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toBottomOf="@id/d35"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="6dp"
                        android:background="@color/bluish_cyan"/>

                </androidx.constraintlayout.widget.ConstraintLayout>

                <!-- map 3 -->
                <ImageView
                    android:id="@+id/map3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:background="@drawable/line_30_36"
                    android:backgroundTint="@color/white"
                    android:contentDescription="@string/map3" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <!-- g3 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/entrance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="@drawable/entrance"
                    android:contentDescription="@string/entrance" />

            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!-- Button -->
    <Button
        android:id="@+id/button"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/parkmap"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:backgroundTint="?attr/mainbutton"
        android:fontFamily="@font/product_sans_bold"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:textAllCaps="false"
        android:text="@string/choose_area"/>

</androidx.constraintlayout.widget.ConstraintLayout>